<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.TardisWizard" name="Tardis Wizard" version="2.0.9" provider-name="TheDoctor">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="plugin.video.stargate-streams" version="1.0.5"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Tardis Wizard</summary>
    <description lang="en">Tardis Wizard allows you to get access to the latest Tardis Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>
